#include <stdio.h>
#include <math.h>

struct Point {
    int x;
    int y;
};

double distanceBetweenPoints(const struct Point *p1, const struct Point *p2) {
    double distance = sqrt(pow(p2->x - p1->x, 2) + pow(p2->y - p1->y, 2));
    return distance;
}

int main() {
    struct Point p1 = {3, 4};
    struct Point p2 = {6, 8};

    double distance = distanceBetweenPoints(&p1, &p2);

    printf("%.2f\n", distance);

    return 0;
}
